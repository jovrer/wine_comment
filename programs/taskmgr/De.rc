IDR_PROCESS_PAGE_CONTEXT MENU DISCARDABLE
BEGIN
    POPUP "DUMMY"
    BEGIN
        MENUITEM "Prozeß &beenden",                ID_PROCESS_PAGE_ENDPROCESS
        MENUITEM "Beende Prozeß&baum",           ID_PROCESS_PAGE_ENDPROCESSTREE

        MENUITEM "&Debuggen",                      ID_PROCESS_PAGE_DEBUG
